{
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "env": {
    "CI": "false",
    "NODE_VERSION": "18.x"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
